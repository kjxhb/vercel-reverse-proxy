{
  "rewrites": [
    { "source": "/:match/:url*", "destination": "https://:match/:url*" },
    { "source": "/:match/:url*/", "destination": "https://:match/:url*/" },
    { "source": "/raw/:match/:url*/", "destination": "https://raw.githubusercontent.com:match/:url*/" }
    { "source": "/:match/:url*", "destination": "http://:match/:url*" },
    { "source": "/:match/:url*/", "destination": "http://:match/:url*/" }
  ]
}
